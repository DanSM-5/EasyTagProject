@model CreateUserViewModel

<form asp-controller="Admin" asp-action="ChangePasswordAdmin" method="post">

    <partial name="UserPasswordInputPartial" for="@Model" />

    <div>
        <button type="submit" class="btn btn-primary">Change Password</button>
    </div>
    <input asp-for="ReturnUrl" type="hidden" />
    <input asp-for="Id" type="hidden" />

    <input asp-for="Role" type="hidden" />
    <input asp-for="UserName" type="hidden" />
    <input asp-for="Email" type="hidden" />
    <input asp-for="PhoneNumber" type="hidden" />
    <input asp-for="FirstName" type="hidden" />
    <input asp-for="LastName" type="hidden" />
</form>
<br />

